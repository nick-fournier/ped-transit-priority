@article{Sanchez2008,
abstract = {By all appearances, the circumstances surrounding employment and income distribution in the United States have remained notably the same over the past 30-40 years. At the same time, policies for improving the conditions of low-income persons have remained relatively unchanged. Relevant published accounts continue to cite poorly integrated residential and employment location patterns and poor public transportation service as critical obstacles to improving the economic and social conditions of low-income persons. The relationship between poverty and public transportation was researched extensively during the late 1960s and the early 1970s; however, little recognition has been given to these efforts by more recent research efforts. To learn from the past we should review public transportation policies from 1960 to 2000 to highlight federal policies that affected urban areas during this time period, especially in relation to low-income transportation mobility. {\textcopyright} 2008 Elsevier Ltd. All rights reserved.},
    author = {Sanchez, Thomas W.},
    doi = {10.1016/j.tra.2008.01.011},
    issn = {09658564},
    journal = {Transportation Research Part A: Policy and Practice},
    keywords = {Low income,Poverty,Public transportation},
    month = {jun},
    number = {5},
    pages = {833--841},
    title = {{Poverty, policy, and public transportation}},
    volume = {42},
    year = {2008}
}
